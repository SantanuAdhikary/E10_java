import java.util.Scanner;
class Primenumber 
{
	static Scanner sc = new Scanner(System.in);
	public static void main(String[] args) 
	{
		System.out.print("enter one number");
		int n = sc.nextInt();
		
		int count = 0 ;
		for(int i=1 ; i<=n ; i++)
		{
			if(n % i ==0)
			{
				System.out.println(i);
				count++;
			}
				
				
		}
		
		System.out.println(count);
	}
}
