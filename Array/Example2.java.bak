class Example2 
{
	public static void main(String[] args) 
	{
		
		int arr[] = {20,10,80,45,67};
		
		System.out.println("first value : "+arr[0]);
		System.out.println("second value : "+arr[1]);
		System.out.println("third value : "+arr[2]);
		
		// how to know the length of the array 
		
		int n = arr.length;
		
		System.out.println("length of the array is : "+n);
		
		
		// how to print or how to access all the value of the array
		
		for(int i=0 ; i<arr.length; i++)
		{
			System.out.println(arr[i]);
		}
		
	}
}
